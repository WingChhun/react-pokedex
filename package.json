{
  "name": "react-pokedex",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3000",
  "dependencies": {
    "google-maps-react": "^2.0.2",
    "lodash": "^4.17.19",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.2",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "test-watch": "npm run test -- --watch",
    "build-css": "node-sass-chokidar    --include-path ./src/sass ./src/styles/main.scss ./src/main.css --include-path ./src/styles  -o",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/styles/main.scss ./src/styles/main.css --watch --recursive",
    "dev": "concurrently \"npm run watch-css\" \"npm run start\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WingChhun/react-pokedex"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest-fetch-mock": "^2.1.2",
    "node-sass-chokidar": "^1.3.5"
  },
  "prettier": {
    "singleQuote": true
  }
}
